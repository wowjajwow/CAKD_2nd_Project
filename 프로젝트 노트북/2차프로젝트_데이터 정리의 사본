nplace=True)"],"metadata":{"id":"eVH7vOtA_ZGa"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["# 엑셀로 저장\n","data_6.to_excel('/content/drive/MyDrive/KDT/2차프로젝트/sample.xlsx')"],"metadata":{"id":"yqY_vXR5MBkQ"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["total"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":424},"id":"ZzkzK8fC7Sv9","executionInfo":{"status":"ok","timestamp":1671653721697,"user_tz":-540,"elapsed":489,"user":{"displayName":"hyung joong kim","userId":"17115953432759157922"}},"outputId":"4590f2b6-d269-4b3d-90bd-0d82136cd9f5"},"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["         제휴사    영수증번호  대분류코드  중분류코드    소분류코드   고객번호  점포코드      구매일자  구매시간  \\\n","0          B  8664000     15   1504  B150401  17218    44  20140222    20   \n","1          B  8664007     15   1504  B150401  11303    44  20140222    13   \n","2          B  8919520     15   1504  B150401  13210    48  20150921    19   \n","3          B  6956422     15   1504  B150401  13823    12  20140813    18   \n","4          B  6957156     15   1504  B150401  18970    12  20140831    22   \n","...       ..      ...    ...    ...      ...    ...   ...       ...   ...   \n","28593025   B  7269975     14   1415  B141503    134    16  20141130    15   \n","28593026   B  7946814     68   6802  B680209  18484    26  20141227    14   \n","28593027   B  7777110      7    702  B070203   5043    21  20151012    22   \n","28593028   B  7579178      7    703  B070305   4073    18  20140125    20   \n","28593029   B  8123279     36   3607  B360704  11509    29  20150821    13   \n","\n","          구매금액     중분류명      소분류명    연도   월    연도_월  \n","0         2420     탄산음료       사이다  2014   2  201402  \n","1         2400     탄산음료       사이다  2014   2  201402  \n","2         5250     탄산음료       사이다  2015   9  201509  \n","3         2250     탄산음료       사이다  2014   8  201408  \n","4         4900     탄산음료      